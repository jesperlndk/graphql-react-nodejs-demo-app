{
    "extends": "airbnb-base",
    "env": {
        "jest": true
    },
    "rules": {
        "camelcase": "off",
        "global-require": "off",
        "implicit-arrow-linebreak": "off",
        "max-len": "off",
        "no-console": "off",
        "no-plusplus": "off",
        "no-underscore-dangle": "off",
        "object-curly-newline": "off"
    }
}